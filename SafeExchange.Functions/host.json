{
  "version": "2.0",
  "logging": {
    "logLevel": {
      "default": "Information",
      "SafeExchange.Core": "Information",
      "System.Net.Http.HttpClient": "Warning"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    },
    "extensions": {
      "queues": {
        "maxPollingInterval": "00:00:15",
        "messageEncoding": "Base64"
      }
    }
  }
}